<h2>Sightings:</h2>
  <% @sightings.each do |sighting| %>
  <p>
    <h3><a href="/birds/<%= sighting.bird.slug %>"><%= sighting.bird.name %></a></h3>
    <h4><a href="/users/<%= sighting.user.slug %>"><%= sighting.user.username %></a></h4>
    <ul>
      <li>Family: <%= sighting.bird.family %></li>
      <li>Location: <%= sighting.location %></li>
      <li>Date: <%= "#{sighting.date}, #{sighting.time}"%></li>
    </ul>
  </p>
  <% end %>
